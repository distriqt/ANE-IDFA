<extension xmlns="http://ns.adobe.com/air/extension/4.0">
	<id>com.distriqt.IDFA</id>
	<versionNumber>@VERSION@</versionNumber>
	<platforms>
		<platform name="Android-ARM">
			<applicationDeployment>
				<nativeLibrary>distriqt.extension.idfa.android.jar</nativeLibrary>
				<initializer>com.distriqt.extension.idfa.IDFAExtension</initializer>
				<finalizer>com.distriqt.extension.idfa.IDFAExtension</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="Android-x86">
			<applicationDeployment>
				<nativeLibrary>distriqt.extension.idfa.android.jar</nativeLibrary>
				<initializer>com.distriqt.extension.idfa.IDFAExtension</initializer>
				<finalizer>com.distriqt.extension.idfa.IDFAExtension</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="iPhone-ARM">
			<applicationDeployment>
				<nativeLibrary>libIDFA.a</nativeLibrary>
				<initializer>IDFAExtInitializer</initializer>
				<finalizer>IDFAExtFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="iPhone-x86">
			<applicationDeployment>
				<nativeLibrary>libIDFA.a</nativeLibrary>
				<initializer>IDFAExtInitializer</initializer>
				<finalizer>IDFAExtFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="default"> 
			<applicationDeployment /> 
		</platform>
	</platforms>
</extension>
